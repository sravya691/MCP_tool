{
  "mcpServers": {
    "duckduckgo-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-duckduckgo@latest"
      ],
      "env": {
        "NODE_ENV": "production"
      }
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "env": {
        "DISPLAY": ":0"
      }
    }
  }
}